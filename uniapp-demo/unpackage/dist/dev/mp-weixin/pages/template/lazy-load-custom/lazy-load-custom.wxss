@import "../../../common/vendor.wxss";

.content {
  font-size: 30rpx;
  line-height: 50rpx;
  color: #666666;
  padding: 30rpx;
}
.content>.title {
  font-size: 32rpx;
  text-indent: 2em;
}
.content>.ul {
  padding-left: 40rpx;
  color: #999999;
}
.uni-media-list-logo {
  position: relative;
}
.uni-media-list-logo wx-image {
  position: absolute;
  left: 0;
  top: 0;
}
.defaul {
  opacity: 1;
  -webkit-transition: opacity 0.4s linear;
  transition: opacity 0.4s linear;
}
.defaul.loaded {
  opacity: 0;
}